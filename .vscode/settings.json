{
    "cSpell.words": [
        "Enock",
        "Yator"
    ],
    "docwriter.style": "JSDoc",
    //exclude large folders from being indexed
    "files.watcherExclude": {
        "**/node_modules":true,
        "**/dist":true,
        "**/.git":true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.hg/store/**": true
    }
}